@model RED.Models.ElectronicDiary.Tests.TestW

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Затвори</span></button>
    <i class="fa fa-area-chart modal-icon"></i>
    <h4 class="modal-title">Изтрий</h4>
    <small class="font-bold">Сигурен ли сте, че изкате да изтриете тoва изследване?</small>
</div>
<div class="modal-body">
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.TestMethods)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TestMethods)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.AcredetationLevel.Level)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AcredetationLevel.Level)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.TestCategory.Name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TestCategory.Name)
        </dd>
    </dl>
</div>
<div class="modal-footer">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <button type="button" class="btn btn-white" data-dismiss="modal">Отказ</button>
            <input type="submit" value="Изтрий" class="btn btn-primary" />
        </div>
    }
</div>